{
  "name": "@21st-digital/eslint-config",
  "version": "0.0.0-development",
  "description": "21st digital's ESLint config with Prettier support",
  "repository": {
    "type": "git",
    "url": "https://github.com/21stdigital/eslint-config-21st.git"
  },
  "keywords": [
    "eslint",
    "eslintconfig",
    "config",
    "javascript",
    "ecmascript",
    "lint",
    "prettier"
  ],
  "author": "Fabian Pahl <fabian@21st.digital>",
  "license": "MIT",
  "homepage": "https://github.com/21stdigital/eslint-config-21st#readme",
  "bugs": {
    "url": "https://github.com/21stdigital/eslint-config-21st/issues"
  },
  "main": "index.js",
  "scripts": {
    "commit": "cz",
    "lint": "eslint .",
    "lint:fix": "eslint --fix",
    "semantic-release": "semantic-release",
    "prepare": "husky install"
  },
  "dependencies": {
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-config-standard-jsx": "^10.0.0",
    "eslint-config-standard-react": "^11.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.2.1",
    "@commitlint/config-conventional": "^13.2.0",
    "babel-eslint": "^10.1.0",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.24.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^7.0.2",
    "prettier": "^2.4.1",
    "semantic-release": "^18.0.0"
  },
  "peerDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^8.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "prettier": "^2.4.1"
  },
  "release": {
    "branches": [
      "main"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
    }
  }
}
